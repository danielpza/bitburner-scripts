{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowUmdGlobalAccess": true,
    "jsx": "react",
    "jsxFactory": "eval(\"window\").React.createElement",
    "jsxFragmentFactory": "eval(\"window\").React.Fragment"
  }
}
